	if (!is.data.frame(df)) return(df)
	nrow <- nrow(df)
	# cut a range if it is not empty. 
	# Quiet noop else. 
	# Does not cut single lines.
	 cutrng <- function(cutfrom,cutto){
	 	if (cutfrom<cutto){
		df[cutfrom,] <- NA
		if (!is.null(rownames(df))) rownames(df)[cutfrom] <- "< cut >"
		if (!is.null(df$name)) df$name[cutfrom] <- ""
		
